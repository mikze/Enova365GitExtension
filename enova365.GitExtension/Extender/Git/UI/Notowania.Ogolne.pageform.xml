<?xml version="1.0"?>
<DataForm xmlns="http://www.enova.pl/schema/form.xsd" Priority="100">
  <Page CaptionHtml="Commits" DataContext="{DataSource}">
    <Group CaptionHtml="All commits">
      <Row>
        <Stack Width="200" Height="*">
          <Grid EditValue="{AllCommits}" Width="150" Height="*" >
            <Field CaptionHtml="Author" Width="20" EditValue="{name}" />
            <Field CaptionHtml="Sha" Width="20" EditValue="{sha}" />
            <Field CaptionHtml="Message" Width="20" EditValue="{message}" />
            <Field CaptionHtml="Branch" Width="20" EditValue="{branch}" />
            <Field CaptionHtml="Date" Width="20" EditValue="{date}" />
          </Grid>
        </Stack>
      </Row>
    </Group>
  </Page>
</DataForm>