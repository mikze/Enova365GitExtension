<?xml version="1.0"?>
<DataForm xmlns="http://www.enova.pl/schema/form.xsd" Priority="100">
  <Page CaptionHtml="Commits" DataContext="{DataSource}">
    <Group CaptionHtml="All commits">
      <Row>
        <Stack Width="200" Height="*">
          <Grid EditValue="{ContributorCommitsPerDay}" Width="150" Height="*" >
            <Field CaptionHtml="date" Width="20" EditValue="{date}" />
            <Field CaptionHtml="count" Width="20" EditValue="{count}" />
          </Grid>
        </Stack>
      </Row>
    </Group>
  </Page>
</DataForm>